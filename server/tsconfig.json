{
  "compilerOptions": {
    "target": "es2016",        
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/swagger_output.json",
    "src/**/*"
  ],
}
